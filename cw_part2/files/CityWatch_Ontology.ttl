#    Object Properties
#################################################################

###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasAccidentType
:hasAccidentType rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :TrafficAccident ;
                 rdfs:range :TrafficAccidentType ;
                 rdfs:comment "indicates classification at the time of the accident." ;
                 rdfs:label "hasAccidentType" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasAlignment
:hasAlignment rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain <https://dbpedia.org/ontology/https://dbpedia.org/ontology/Road> ;
              rdfs:range :RoadAlignment ;
              rdfs:comment "The alignment of the road where the accident occurred (e.g., straight, curved)." ;
              rdfs:label "hasAlignment" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasDriverRelatedCause
:hasDriverRelatedCause rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasTrafficAccidentCause ;
                       rdf:type owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :TrafficAccident ;
                       rdfs:range :DriverRelatedCause ;
                       rdfs:comment "Indicates the driver-related conditions at the time of an accident." .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasEnvironmentalCause
:hasEnvironmentalCause rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasTrafficAccidentCause ;
                       rdf:type owl:AsymmetricProperty ,

                        rdfs:domain :TrafficAccident ;
                        rdfs:range :VehicleRelatedCause ;
                        rdfs:comment "Indicates the vehicle-related conditions at the time of an accident." .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasWeatherCondition
:hasWeatherCondition rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:FunctionalProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain :TrafficAccident ;
                     rdfs:range :WeatherCondition ;
                     rdfs:comment "Indicates the weather conditions at the time of the accident." ;
                     rdfs:label "hasWeatherCondition" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#involvesPerson
:involvesPerson rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :TrafficAccident ;
                rdfs:range :Person ;
                rdfs:comment "Indicates that a person (such as a driver, passenger, or pedestrian) is involved in the accident." ;
                rdfs:label "involvesPerson" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#involvesVehicle
:involvesVehicle rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :TrafficAccident ;
                 rdfs:range :Vehicle ;
                 rdfs:comment "Indicates that a vehicle (such as a car, bus, truck, motorcycle, or bicycle) is involved in the accident." ;
                 rdfs:label "involvesVehicle" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#occurAt
:occurAt rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ,
                  owl:AsymmetricProperty ;
         rdfs:domain :TrafficAccident ;
         rdfs:range <https://dbpedia.org/ontology/https://dbpedia.org/ontology/Road> ;
         rdfs:comment "Indicates the location involvement in the at the time of the accident." ;
         rdfs:label "occurAt" .


#################################################################

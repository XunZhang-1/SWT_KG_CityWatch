                                  owl:IrreflexiveProperty ;
                         rdfs:domain :TrafficAccident ;
                         rdfs:range :TrafficAccidentCause ;
                         rdfs:comment "indicates primary cause at the time of the accident." ;
                         rdfs:label "hasTrafficAccidentCause" .


###  http://www.city.ac.uk/inm713-in3067/2025/CityWatch#hasTrafficControlDevice
:hasTrafficControlDevice rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ,
                                  owl:AsymmetricProperty ;
                         rdfs:domain :TrafficAccident ;
                         rdfs:range :TrafficControlDevice ;
                         rdfs:comment "Indicates the type of traffic control device involved (e.g., traffic light, sign)." ;
                         rdfs:label "hasTrafficControlDevice" .


